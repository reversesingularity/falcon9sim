<?xml version="1.0"?>
<!--
  RCS Thruster Engine Definition for Falcon 9 Second Stage
  Reaction Control System (RCS) for attitude control and fine maneuvering
  
  Specifications (typical for spacecraft RCS):
  - Thrust: 400 N (90 lbf) per thruster
  - Specific Impulse: 240 s (cold gas or monoprop)
  - Propellant: Nitrogen (cold gas) or Hydrazine (monoprop)
  - Used for attitude control, station-keeping, and fine orbital adjustments
-->

<rocket_engine name="RCS_thruster">
  <isp unit="SEC"> 240.0 </isp>
  <builduptime> 0.01 </builduptime>  <!-- Nearly instantaneous response -->
  <shutdowntime> 0.01 </shutdowntime>
  
  <!-- Maximum thrust per RCS thruster -->
  <maxthrust unit="LBS"> 90.0 </maxthrust>
  
  <!-- RCS thrusters are typically on/off, no throttling -->
  <minthrottle> 0.0 </minthrottle>
  
  <!-- Propellant consumption rate -->
  <!-- mdot = thrust / (Isp * g0) = 90 / (240 * 32.174) = 0.0116 lbs/sec -->
  <propellant_flow_rate unit="LBS/SEC"> 0.0116 </propellant_flow_rate>
  
  <!-- RCS thrusters can be restarted many times -->
  <restart_capability> true </restart_capability>
  <max_restarts> 10000 </max_restarts>  <!-- Essentially unlimited -->
  
  <!-- RCS performance is relatively constant with altitude -->
  <!-- (cold gas or monoprop doesn't vary much with back-pressure) -->
  <thrust_table name="altitude_thrust_factor">
    <independentVar lookup="row">position/h-agl-ft</independentVar>
    <tableData>
          0.0    0.95   <!-- Slight reduction at sea level -->
      10000.0    0.98
      50000.0    1.00   <!-- Full performance in vacuum -->
     100000.0    1.00
     300000.0    1.00
    </tableData>
  </thrust_table>
  
  <!-- ISP is relatively constant for cold gas/monoprop systems -->
  <isp_table name="altitude_isp_factor">
    <independentVar lookup="row">position/h-agl-ft</independentVar>
    <tableData>
          0.0    0.98
      10000.0    0.99
      50000.0    1.00
     100000.0    1.00
     300000.0    1.00
    </tableData>
  </isp_table>
  
  <!-- RCS thrusters typically don't gimbal -->
  <gimbal_pitch_limit unit="DEG"> 0.0 </gimbal_pitch_limit>
  <gimbal_yaw_limit unit="DEG"> 0.0 </gimbal_yaw_limit>
  
  <!-- Default location (overridden by individual thruster definitions) -->
  <location unit="M">
    <x> 0.0 </x>
    <y> 0.0 </y>
    <z> 0.0 </z>
  </location>
  
  <!-- Default orientation -->
  <orientation unit="DEG">
    <pitch> 0.0 </pitch>
    <yaw> 0.0 </yaw>
    <roll> 0.0 </roll>
  </orientation>
  
</rocket_engine>